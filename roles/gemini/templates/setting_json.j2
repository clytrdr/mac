{
  "context": {
    "fileName": "AGENTS.md"
  },
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ]
    },
    "jetbrains": {
      "url": "http://localhost:64342/sse"
    },
    "terraform": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "hashicorp/terraform-mcp-server"
      ]
    }
  },
  "general": {
    "preferredEditor": "vim",
    "previewFeatures": true,
    "sessionRetention": {
      "warningAcknowledged": true,
      "enabled": true,
      "maxAge": "30d"
    }
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "ui": {
    "theme": "ANSI",
    "footer": {
      "hideModelInfo": false
    },
    "showModelInfoInChat": true
  },
  "hasSeenIdeIntegrationNudge": true,
  "ide": {
    "hasSeenNudge": true
  }
}
